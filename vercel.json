{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/news",
      "dest": "/api/news.js"
    },
    {
      "src": "/api/events",
      "dest": "/api/events.js"
    },
    {
      "src": "/api/noticias",
      "dest": "/api/noticias.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
